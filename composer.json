{
  "name": "eastown/laravel-api-token",
  "description": "API Token authentication for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Eastown",
      "email": "eastown111@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5|^6|^7|^8",
    "illuminate/database": "^5|^6|^7|^8",
    "illuminate/http": "^5|^6|^7|^8"
  },
  "autoload": {
    "psr-4": {
      "Eastown\\ApiToken\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Eastown\\ApiToken\\Providers\\TokenServiceProvider"
      ]
    }
  }
}
